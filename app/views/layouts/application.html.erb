<!DOCTYPE html>
<html>
  <head>
    <title>Event Manager</title>
    <%= stylesheet_link_tag    "application", :media => "all" %>
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>

  </head>
  <body>

    <!-- Begin Wrapper -->
    <div id="wrapper">

      <!-- Begin Header -->
      <div id="header">

        Header

      </div>
      <!-- End Header -->

      <!-- Begin Navigation -->
      <div id="nav">
        
        <%= link_to 'Home', sponsors_path %>
        <%= link_to 'Exhibitors', exhibitors_path %>
        <%= link_to 'Meetings', meetings_path %>
        <%= link_to 'Venues', venues_path %>
        <%= link_to 'Exhibitor Categories', exhibitorcategories_path %>
        <%= link_to 'Contests', contests_path %>

      </div>
      <!-- End Navigation -->

      <!-- Begin Left Column -->
      <div id="mainbody">

        <%= yield %>

      </div>
      <!-- End Left Column -->

      <!-- Begin Footer -->
      <div id="footer">

        This is the footer

      </div>
      <!-- End Footer -->

    </div>
    <!-- End Wrapper -->

  </body>
</html>
